{
  "name": "corallum-enterprise",
  "version": "2.0.0",
  "description": "Enterprise AI-powered workflow automation platform with multi-tenant, SSO, RBAC, and 99.9% uptime",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate:create": "node scripts/migrate.js create",
    "migrate:up": "node scripts/migrate.js up",
    "migrate:down": "node scripts/migrate.js down",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docker:build": "docker build -t corallum-enterprise .",
    "docker:run": "docker run -p 8003:8003 corallum-enterprise"
  },
  "keywords": [
    "workflow",
    "automation",
    "ai",
    "enterprise",
    "multi-tenant",
    "sso",
    "rbac",
    "langchain",
    "inngest",
    "production-ready"
  ],
  "author": "Corallum Enterprise Team",
  "license": "MIT",
  "dependencies": {
    "@langchain/community": "^0.0.20",
    "@langchain/core": "^0.1.26",
    "@langchain/openai": "^0.0.14",
    "@langchain/textsplitters": "^0.0.0",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "envalid": "^8.1.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "inngest": "^3.7.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.0.208",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node-pg-migrate": "^8.0.4",
    "pg": "^8.11.3",
    "pino": "^10.1.1",
    "pino-http": "^11.0.0",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.14",
    "tsyringe": "^4.10.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/bcrypt": "^5.0.1",
    "@types/compression": "^1.7.4",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.7",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "bcryptjs": "^3.0.3",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/corallum/enterprise"
  },
  "bugs": {
    "url": "https://github.com/corallum/enterprise/issues"
  },
  "homepage": "https://corallum.com/enterprise"
}
